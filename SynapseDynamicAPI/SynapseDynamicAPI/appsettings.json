{
  "SynapseCore": {
    "ConnectionStrings": {
      "SynapseDataStore": "Server=@@SERVERNAME@@;User Id=@@USERNAME@@;Password=@@PASSWORD@@;Database=synapse;",
      "SynapseIdentityStore": "Server=@@SERVERNAME@@;User Id=@@SERVERNAME@@;Password=@@PASSWORD@@;Database=synapseidentity;"
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "",
    "TenantId": "",
    "ClientId": ""
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Tokens": {
    "Key": "INSERT_YOUR_OWN_KEY_HERE",
    "Issuer": "http://YOUR_URL:50764",
    "Timeout": 10
  },
  "OutboundInterface": {
    "SendingApplicationName": "CP"
  }
}
